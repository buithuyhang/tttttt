                                    dataSource:response1.ListTraVe,
                                    filter: "contains",
                                    suggest: true,
                                    template:
                                        "<input type='checkbox' id='chk_cbbTuyen_BCBenKhongHoatDong_#=data.LT_IdLuongTuyen #' onclick='UpdateIdinHF(this);' value='#=data.LT_IdLuongTuyen #' name='cbbTuyen_BCBenKhongHoatDong' />" +
