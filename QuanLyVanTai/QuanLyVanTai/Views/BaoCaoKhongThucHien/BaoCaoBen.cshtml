                        <input id="cbbTuyen_BCBenKhongHoatDong" placeholder="CHỌN TUYẾN..." style="width:100%; max-width:100000px;">
                        <input id="hf_cbbTuyen_BCBenKhongHoatDong" type="hidden">
                    </div>
                </div>
            </div>
            <hr style="margin-bottom:10px;margin-top:10px;" />
